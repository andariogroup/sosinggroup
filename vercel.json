{
  "images": {
    "domains": ["localhost", "www.sosinggroup.com", "sosinggroup.com"],
    "formats": ["image/webp", "image/avif"],
    "minimumCacheTTL": 60
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install"
}
